				</not>
				<not>
					<equals>
						<property>/controls/elec_panel/start-mode</property>
						<value>0</value>
					</equals>
				</not>
			</condition>
			<position>
				<x>0</x>
				<y>-5.7</y>
				<z>1.1</z>
			</position>
			<reference-dist>50.0</reference-dist>
			<max-dist>500.0</max-dist>
		</engine>

