<?xml version="1.0"?>

<PropertyList>

	<fx>
		<caution>
			<name>Caution</name>
			<path>Aircraft/ATR72/Sounds/caution.wav</path>
			<mode>once</mode>
			<condition>
				<and>
					<equals>
						<property>/aircraft/ccas/master-caution</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/systems/electric/outputs/avionics</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
			<volume>
				<property>sim/sound/effects/volume</property>
				<factor>0.5</factor>
			</volume>
			<position>
				<x>0.00</x>
				<y>0.00</y>
				<z>0.00</z>
			</position>
			<reference-dist>10.0</reference-dist>
			<max-dist>100.0</max-dist>
		</caution>
		
		<ap_off>
			<name>AP off</name>
			<path>Aircraft/ATR72/Sounds/ap_off.wav</path>
			<mode>once</mode>
			<condition>
				<and>
					<equals>
						<property>/aircraft/afcs/ap-master</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/systems/electric/outputs/avionics</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
			<volume>
				<factor>5</factor>
			</volume>
			<position>
				<x>0.00</x>
				<y>0.00</y>
				<z>0.00</z>
			</position>
			<reference-dist>10.0</reference-dist>
			<max-dist>100.0</max-dist>
		</ap_off>
		
		<warning_500>
			<name>warning_500</name>
			<path>Aircraft/ATR72/Sounds/GPWS/500.wav</path>
			<mode>once</mode>
			<condition>
				<and>
					<equals>
						<property>/aircraft/cockpitSounds/altitudeDirection</property>
						<value>descend</value>
					</equals>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<less-than-equals>
							<property>/position/altitude-agl-ft</property>
							<value>500</value>
					</less-than-equals>
				</and>
			</condition>
			<volume>
				<factor>10</factor>
			</volume>
			<position>
				<x>0.00</x>
				<y>0.00</y>
				<z>0.00</z>
			</position>
			<reference-dist>10.0</reference-dist>
			<max-dist>100.0</max-dist>
		</warning_500>
	
		<warning_bankangle>
			<name>warning_bankangle</name>
			<path>Aircraft/ATR72/Sounds/GPWS/bank-angle.wav</path>
			<mode>loop</mode>
			<condition>
				<and>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<greater-than-equals>
						<property>/aircraft/cockpitSounds/bank-angle-warning</property>
						<value>1</value>
					</greater-than-equals>
				</and>
			</condition>
			<volume>
				<factor>10</factor>
			</volume>
			<position>
				<x>0.00</x>
				<y>0.00</y>
				<z>0.00</z>
			</position>
			<reference-dist>10.0</reference-dist>
			<max-dist>100.0</max-dist>
		</warning_bankangle>
	
		<wind>
			<name>wind</name>
			<mode>looped</mode>
			<path>Aircraft/ATR72/Sounds/wind.wav</path>
			<property>/velocities/airspeed-kt</property>
			<volume>
				<property>/position/altitude-ft</property>
				<factor>-0.000015</factor>
				<offset>1.0</offset>
				<min>0.1</min>
				<max>1.0</max>
			</volume>
			<volume>
				<property>/velocities/airspeed-kt</property>
				<factor>0.0020</factor>
				<min>0.03</min>
				<max>0.25</max>
			</volume>
			<pitch>
				<property>/velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<offset>1.25</offset>
			</pitch>
			<reference-dist>1.0</reference-dist>
			<max-dist>2.0</max-dist>
		</wind>

		<roll>
			<name>Roll</name>
			<path>Aircraft/ATR72/Sounds/roll.wav</path>
			<mode>looped</mode>
			<condition>
				<property>sim/current-view/internal</property>
				<or>
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
				</or>
			</condition>
			<volume>
				<property>/velocities/groundspeed-kt</property>
				<type>log</type>
				<factor>0.20</factor>
			</volume>
			<pitch>
				<property>/velocities/groundspeed-kt</property>
				<type>log</type>
				<factor>0.28</factor>
				<min>0.1</min>
				<max>1.5</max>
			</pitch>
		</roll>
		
		<Wheels>
			<name>nose Wheel Touchdown</name>
			<path>Aircraft/ATR72/Sounds/touch.wav</path>
			<condition>
				<property>/gear/gear[0]/wow</property>
			</condition>
			<volume>
				<property>sim/sound/effects/volume</property>
			</volume>
			<volume>
				<property>/velocities/speed-down-fps</property>
				<factor>0.10</factor>
			</volume>
			<volume>
				<property>/velocities/groundspeed-kt</property>
				<factor>0.01</factor>
			</volume>
			<volume>
				<internal>dt_stop</internal>
				<factor>0.05</factor>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>/velocities/groundspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.2</offset>
			</pitch>
			<position>
				<x>-11</x>
				<y>0</y>
				<z>-1.3</z>
			</position>
			<reference-dist>20.0</reference-dist>
			<max-dist>500.0</max-dist>
		</Wheels>
		
		<Wheels>
			<name>left main Wheel Touchdown</name>
			<path>Aircraft/ATR72/Sounds/touch.wav</path>
			<condition>
				<property>/gear/gear[1]/wow</property>
			</condition>
			<volume>
				<property>sim/sound/effects/volume</property>
			</volume>
			<volume>
				<property>/velocities/speed-down-fps</property>
				<factor>0.10</factor>
			</volume>
			<volume>
				<property>/velocities/groundspeed-kt</property>
				<factor>0.01</factor>
			</volume>
			<volume>
				<internal>dt_stop</internal>
				<factor>0.0025</factor>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>/velocities/groundspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.2</offset>
			</pitch>
			<position>
				<x>0</x>
				<y>-2</y>
				<z>-1.4</z>
			</position>
			<reference-dist>20.0</reference-dist>
			<max-dist>500.0</max-dist>
		</Wheels>
		
		<Wheels>
			<name>right main Wheel Touchdown</name>
			<path>Aircraft/ATR72/Sounds/touch.wav</path>
			<condition>
				<property>/gear/gear[2]/wow</property>
			</condition>
			<volume>
				<property>sim/sound/effects/volume</property>
			</volume>
			<volume>
				<property>/velocities/speed-down-fps</property>
				<factor>0.10</factor>
			</volume>
			<volume>
				<property>/velocities/groundspeed-kt</property>
				<factor>0.01</factor>
			</volume>
			<volume>
				<internal>dt_stop</internal>
				<factor>0.05</factor>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>/velocities/groundspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.2</offset>
			</pitch>
			<position>
				<x>0</x>
				<y>2</y>
				<z>-1.4</z>
			</position>
			<reference-dist>20.0</reference-dist>
			<max-dist>500.0</max-dist>
		</Wheels>

		<flap>
			<name>flaps</name>
			<mode>in-transit</mode>
			<path>Aircraft/ATR72/Sounds/flaps.wav</path>
			<property>/surface-positions/flap-pos-norm</property>
			<volume>
				<property>sim/sound/effects/volume</property>
				<max>0.8</max>
			</volume>
			<reference-dist>5.0</reference-dist>
			<max-dist>50.0</max-dist>
		</flap>

		<gear>
			<name>gear</name>
			<mode>in-transit</mode>
			<path>Aircraft/ATR72/Sounds/gears.wav</path>
			<property>/gear/gear[0]/position-norm</property>
			<volume>
				<property>sim/sound/effects/volume</property>
			</volume>
			<pitch>
				<internal>dt_play</internal>
				<offset>0.7</offset>
				<factor>-0.001</factor>
			</pitch>
			<reference-dist>5.0</reference-dist>
			<max-dist>50.0</max-dist>
		</gear>

  <!-- Seatbelt/No smoking signs -->
		<seatbelt>
			<name>Seatbelt sign</name>
			<path>Aircraft/ATR72/Sounds/cabinalert.wav</path>
			<mode>once</mode>
			<condition>
				<or>
					<property>sim/sound/no-smoking-sign</property>
					<property>sim/sound/seatbelt-sign</property>
				</or>
				<property>sim/current-view/internal</property>
			</condition>
			<volume>
				<property>sim/sound/effects/volume</property>
			</volume>
			<position>
				<x>0.00</x>
				<y>0.00</y>
				<z>0.00</z>
			</position>
			<reference-dist>10.0</reference-dist>
			<max-dist>50.0</max-dist>
		</seatbelt>

		<stall>
			<name>stall</name>
			<mode>looped</mode>
			<path>/Aircraft/ATR72/Sounds/stall.wav</path>
			<type>avionics</type>
			<condition>
				<property>sim/sound/stall-horn</property>
			</condition>
			<volume>
				<property>sim/sound/effects/volume</property>
				<!--offset>1</offset-->
			</volume>
			
			<reference-dist>10</reference-dist>
			<max-dist>100.0</max-dist>
		</stall>

    <!-- Copilot -->
    
    <copilot>
	<name>v1</name>
