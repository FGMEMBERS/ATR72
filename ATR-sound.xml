    <!-- Engine Sounds -->
    
 <!-- =============================================================== -->
 <!-- ENGINE OUTSIDE:                                                 -->
 <!-- =============================================================== -->

		<engine>
			<name>left engine</name>
			<path>Aircraft/ATR72/Sounds/props.wav</path>
			<mode>looped</mode>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
			</condition>
			<volume>
				<property>engines/engine[0]/thruster/prop_rpm</property>
				<factor>0.0028571</factor>
				<min>0.0</min>
				<max>1</max>
			</volume>
			<pitch>
				<property>/engines/engine[1]/n1</property>
				<offset>0</offset>
				<factor>0.02</factor>
				<min>0.0</min>
				<max>2.0</max>
			</pitch>
			<position>
				<x>0</x>
				<y>-5.7</y>
				<z>1.1</z>
			</position>
			<reference-dist>50.0</reference-dist>
			<max-dist>500.0</max-dist>
		</engine>
		
		<engine>
			<name>left engine starter</name>
			<path>Aircraft/ATR72/Sounds/startup.wav</path>
			<mode>once</mode>
			<condition>
				<greater-than>
					<property>/engines/engine[0]/n1</property>
					<value>2</value>
				</greater-than>
				<less-than-equals>
					<property>/engines/engine[0]/n1</property>
					<value>18</value>
				</less-than-equals>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<not>
					<equals>
						<property>/controls/elec_panel/start-mode</property>
						<value>0</value>
					</equals>
				</not>
			</condition>
			<position>
				<x>0</x>
				<y>-5.7</y>
				<z>1.1</z>
			</position>
			<reference-dist>50.0</reference-dist>
			<max-dist>500.0</max-dist>
		</engine>
		
		<engine>
			<name>left engine starter loop</name>
			<path>Aircraft/ATR72/Sounds/startup_loop.wav</path>
			<mode>looped</mode>
