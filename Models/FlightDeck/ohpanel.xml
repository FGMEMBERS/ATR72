	<!-- Engine Starter Panel -->
	
	<animation>
		<type>pick</type>
		<object-name>eng1start</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>
				setprop("/controls/engines/engine/starter", 1);
				</script>
			</binding>
			<mod-up>
				<binding>
					<command>nasal</command>
					<script>
					setprop("/controls/engines/engine/starter", 0);
					</script>
				</binding>
			</mod-up>
		</action>
	</animation>
	
