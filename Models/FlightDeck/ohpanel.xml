	<animation>
		<type>pick</type>
		<object-name>startmode</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>/controls/elec_panel/start-mode</property>
				<value>1</value>
				<value>2</value>
				<value>3</value>
				<value>4</value>
				<value>0</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>
				if (getprop("/controls/elec_panel/start-mode") == 0) {
				
					setprop("/controls/electric/engine/generator", 0);
					setprop("/controls/electric/engine[1]/generator", 0);
				
				} elsif (getprop("/controls/elec_panel/start-mode") == 2) {
				
					setprop("/controls/electric/engine/generator", 1);
					setprop("/controls/electric/engine[1]/generator", 0);
				
				} elsif (getprop("/controls/elec_panel/start-mode") == 3) {
				
					setprop("/controls/electric/engine/generator", 0);
					setprop("/controls/electric/engine[1]/generator", 1);
				
				} elsif (getprop("/controls/elec_panel/start-mode") == 4) {
				
					setprop("/controls/electric/engine/generator", 1);
					setprop("/controls/electric/engine[1]/generator", 1);
				
				}
				</script>
			</binding>
		</action>
	</animation>
	
