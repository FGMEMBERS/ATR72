	<!-- Engine Starter Panel -->
	
	<animation>
		<type>pick</type>
		<object-name>eng1start</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>
				setprop("/controls/engines/engine/starter", 1);
				</script>
			</binding>
			<mod-up>
				<binding>
					<command>nasal</command>
					<script>
					setprop("/controls/engines/engine/starter", 0);
					</script>
				</binding>
			</mod-up>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>eng2start</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>
				setprop("/controls/engines/engine[1]/starter", 1);
				</script>
			</binding>
			<mod-up>
				<binding>
					<command>nasal</command>
					<script>
					setprop("/controls/engines/engine[1]/starter", 0);
					</script>
				</binding>
			</mod-up>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>startmode</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>/controls/elec_panel/start-mode</property>
				<value>1</value>
				<value>2</value>
				<value>3</value>
				<value>4</value>
				<value>0</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>
				if (getprop("/controls/elec_panel/start-mode") == 0) {
				
					setprop("/controls/electric/engine/generator", 0);
					setprop("/controls/electric/engine[1]/generator", 0);
				
				} elsif (getprop("/controls/elec_panel/start-mode") == 2) {
				
					setprop("/controls/electric/engine/generator", 1);
					setprop("/controls/electric/engine[1]/generator", 0);
				
				} elsif (getprop("/controls/elec_panel/start-mode") == 3) {
				
					setprop("/controls/electric/engine/generator", 0);
					setprop("/controls/electric/engine[1]/generator", 1);
				
				} elsif (getprop("/controls/elec_panel/start-mode") == 4) {
				
					setprop("/controls/electric/engine/generator", 1);
					setprop("/controls/electric/engine[1]/generator", 1);
				
				}
				</script>
			</binding>
		</action>
	</animation>
	
