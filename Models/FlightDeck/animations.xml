<PropertyList>

<!--=================== Automatic Flight Control System =====================-->

<!-- Knobs Pick and Rotate -->

	<animation>
		<type>pick</type>
		<object-name>hdgknob</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				
					var hdg_prop = "/aircraft/afcs/hdg-setting";
				
					if (359 > getprop(hdg_prop))
						setprop(hdg_prop, getprop(hdg_prop) + 1);
					else
						setprop(hdg_prop, 0);
					
				</script>
			</binding>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					
					var hdg_prop = "/aircraft/afcs/hdg-setting";
				
					if (getprop(hdg_prop) > 0)
						setprop(hdg_prop, getprop(hdg_prop) - 1);
					else
						setprop(hdg_prop, 359);
					
				</script>
			</binding>
		</action>
	</animation>
	
