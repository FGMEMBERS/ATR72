<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

<!--=================== Automatic Flight Control System =====================-->

	<!-- For some reason, a fake animation is required here to enable pilots animation -->
	
	<animation>
		<type>select</type>
		<object-name>fake</object-name>
		<condition>
			<property>/aircraft/anim</property>
		</condition>
	</animation>
	
	<!-- Show Pilots Only in Exterior View -->
	
	<animation>
		<type>select</type>
		<object-name>pilots</object-name>
		<condition>
			<not>
				<property>sim/current-view/internal</property>
			</not>
		</condition>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>ldgL</object-name>
		<property>/controls/ohpanel/ldggearl</property>
		<factor>-0.25</factor>
		<axis>
			<y>1</y>
		</axis>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>ldgN</object-name>
		<property>/controls/ohpanel/ldggearn</property>
		<factor>-0.25</factor>
		<axis>
			<y>1</y>
		</axis>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>ldgR</object-name>
		<property>/controls/ohpanel/ldggearr</property>
		<factor>-0.25</factor>
		<axis>
			<y>1</y>
		</axis>
	</animation>
	
	<!-- Standby-AI -->
	
	<animation>
        <type>textranslate</type>
        <object-name>stby-ai-horizon</object-name>
        <property>orientation/pitch-deg</property>
        <subtype>textranslate</subtype>
        <factor>0.0037</factor>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
    </animation>
    
    <animation>
		<type>rotate</type>
		<object-name>stby-ai-horizon</object-name>
		<property>/orientation/roll-deg</property>
		<factor>-1</factor>
		<axis>
			<x1-m>-11.292</x1-m>
			<y1-m>-0.2268</y1-m>
			<z1-m>0.6441</z1-m>
			<x2-m>-11.3379</x2-m>
			<y2-m>-0.2268</y2-m>
			<z2-m>0.6360</z2-m>
		</axis>
	</animation>
	
<!-- Instrument Settings -->

<animation>
		<type>pick</type>
		<object-name>altimeter-setting</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				
					var alt_setting = getprop("/instrumentation/altimeter/setting-inhg");
					
					setprop("/instrumentation/altimeter/setting-inhg", alt_setting + 0.01);
					
				</script>
			</binding>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					
					var alt_setting = getprop("/instrumentation/altimeter/setting-inhg");
					
					setprop("/instrumentation/altimeter/setting-inhg", alt_setting - 0.01);
					
				</script>
			</binding>
		</action>
	</animation>

<!-- Knobs Pick and Rotate -->

	<animation>
		<type>pick</type>
		<object-name>crs2</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				
					var hdg_prop = "/aircraft/afcs/crs2";
				
					if (359 > getprop(hdg_prop))
						setprop(hdg_prop, getprop(hdg_prop) + 1);
					else
						setprop(hdg_prop, 0);
					
				</script>
			</binding>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					
					var hdg_prop = "/aircraft/afcs/crs2";
				
					if (getprop(hdg_prop) > 0)
						setprop(hdg_prop, getprop(hdg_prop) - 1);
					else
						setprop(hdg_prop, 359);
					
				</script>
			</binding>
		</action>
	</animation>
	
